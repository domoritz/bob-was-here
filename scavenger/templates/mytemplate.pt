<body>
  <h1>Tell everyone that you were here today</h1>
  <ul>
    <li>Bob</li>
    <li>Nick</li>
    <li>John</li>
    <li>Paul</li>
    <li>
      <form action="mockup_submit" method="get" accept-charset="utf-8">
        <input type="text" name="Name" value="" placeholder="Your Name">
        <button type="submit">Add Name</button>
      </form>
    </li>
  </ul>
